{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    // ... other configs, if any
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/app/assets/*"],
      "@components/*": ["src/app/components/*"],
      "@screens/*": ["src/app/views/*"],
      "@common/*": ["src/app/common/*"],
      "@storage/*": ["src/storage/*"],
      "@store/*": ["src/store/*"],
      "@queryclient/*": ["src/query-client/*"]
    }
  }
}
